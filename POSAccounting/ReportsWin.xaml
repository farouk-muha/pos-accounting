<Window x:Class="POSAccounting.ReportsWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POSAccounting"
        mc:Ignorable="d"
        xmlns:rdlcreport="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"  
                          xmlns:p = "clr-namespace:POSAccounting.Properties"
        Title="{x:Static p:Resources.Reports}" Height="300" Width="300"
         WindowState="Maximized"
      >
    <Grid>
        <WindowsFormsHost x:Name="WinFormHost">

            <rdlcreport:ReportViewer x:Name="_reportView" >

            </rdlcreport:ReportViewer>

        </WindowsFormsHost>
    </Grid>
</Window>
